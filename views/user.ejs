<%- include ('partials/header'); -%> 
<div class="container">
<div class="row">
    <div class="col-md-6">
        <img class="img-thumbnail" src="<%= user.avatar %> " alt="">
    </div>
    <div class="col-md-6">
      <div class="well">
          <ul></ul>
          <h1><%= user.username %> </h1>
          <hr>
          <h3><%= user.firstName %> </h3>
          <h3><%= user.lastName %> </h3>
          <h3>  Mail to <a href="http://gmail.com"><%= user.Email %></a> </h3>
          <h2>Owned campgrounds</h2>
        <% campgrounds.forEach(campground => { %>
            <a href="/campgrounds/<%= campground._id %> "><h3><%= campground.name %> </h3></a>
           
         
        <% }) %>     
      </div>

    </div>

</div>
</div>

<%- include ('partials/footer'); -%> 